{
    "manifest_version": 3,
    "name": "Medical AI Assistant",
    "version": "1.0",
    "description": "AI assistant for medical calls",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*-*-*"
            ],
            "js": [
                "content.js"
            ],
            "css": [
                "styles.css"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}